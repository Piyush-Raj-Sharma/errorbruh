{
  "name": "errorbruh",
  "displayName": "ErrorBruh",
  "description": "ErrorBruh makes your coding experience more emotional and hilarious by playing meme-worthy sounds whenever your code fails, crashes, or throws errors. Turn boring error messages into entertaining moments with iconic reactions like the viral FAAH sound.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "errorbruh.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.3"
  },
  "dependencies": {
    "play-sound": "^1.1.6"
  }
}
