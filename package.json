{
  "name": "errorbruh",
  "displayName": "ErrorBruh",
  "description": "ErrorBruh makes your coding experience more emotional and hilarious by playing meme-worthy sounds whenever your code fails, crashes, or throws errors. Turn boring error messages into entertaining moments with iconic reactions like the viral FAAH sound.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.3"
  },
  "dependencies": {
    "play-sound": "^1.1.6"
  },
  "contributes":{
    "configuration":{
      "title": "Error Sounds",
      "properties": {
        "errorSounds.terminalSoundChoice": {
          "type": "string",
          "enum": ["faaah.mp3", "aaag.mp3", "tumIASki.mp3", "none"],
          "enumDescriptions": ["Faaah the tredny one", "amitabbachaaan", "ias ki taiyaari chor do", "Mute Terminal Sound"],
          "default": "faaah.mp3",
          "description": "Choose the sound to play in the terminal when an error occurs."
        },
        "errorSounds.ideSoundChoice": {
          "type": "string",
          "enum": ["faaah.mp3", "aaag.mp3", "tumIASki.mp3", "none"],
          "enumDescriptions": ["Faaah the tredny one", "amitabbachaaan", "ias ki taiyaari chor do", "Mute Debug Sound"],
          "default": "faaah.mp3",
          "description": "Choose the sound to play in the debug console when an error occurs."
        }
      }
    }
  }
}
